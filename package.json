{
  "name": "vue-multi-calendar-picker",
  "version": "1.2.0",
  "dependencies": {
    "@ttbooking/vue-click-outside-directive": "^1.0.3",
    "dayjs": "^1.11.10",
    "vue": "^2.7.14"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.19.1",
    "@popperjs/core": "^2.11.6",
    "@vue/cli-plugin-babel": "~5.0.8",
    "@vue/cli-plugin-eslint": "~5.0.8",
    "@vue/cli-service": "~5.0.8",
    "bootstrap": "^5.2.3",
    "core-js": "^3.26.1",
    "eslint": "^8.28.0",
    "eslint-plugin-vue": "^9.8.0",
    "font-awesome": "^4.7.0",
    "global": "^4.4.0",
    "sass": "^1.56.1",
    "sass-loader": "13.2.0",
    "style-loader": "^3.3.1",
    "vue-template-compiler": "^2.7.14"
  },
  "description": "CalendarPicker based on momentjs",
  "main": "index.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "lint": "vue-cli-service lint",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "rules": {
      "vue/multi-word-component-names": [
        "error",
        {
          "ignores": [
            "Layout"
          ]
        }
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ttbooking/vue-multi-calendar-picker.git"
  },
  "author": "a.romashov",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ttbooking/vue-multi-calendar-picker/issues"
  },
  "homepage": "https://github.com/ttbooking/vue-multi-calendar-picker#readme",
  "packageManager": "yarn@3.3.0"
}
